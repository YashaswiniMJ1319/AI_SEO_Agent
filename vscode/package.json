{
  "publisher": "yashaswinimj1319",
  "name": "ai-seo-agent-vscode",
  "displayName": "AI SEO Agent",
  "description": "VS Code extension for the AI SEO Agent SaaS platform",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Linters",
    "Other"
  ],
  "activationEvents": [
    "onCommand:ai-seo-agent-vscode.login",
    "onCommand:ai-seo-agent-vscode.logout",
    "onCommand:ai-seo-agent-vscode.analyzeFile",
    "onAuthenticationRequest:ai-seo-agent-auth",
    "onUri"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-seo-agent-vscode.login",
        "title": "AI SEO Agent: Login",
        "category": "AI SEO Agent"
      },
      {
        "command": "ai-seo-agent-vscode.logout",
        "title": "AI SEO Agent: Logout",
        "category": "AI SEO Agent"
      },
      {
        "command": "ai-seo-agent-vscode.analyzeFile",
        "title": "AI SEO Agent: Analyze Current File",
        "category": "AI SEO Agent"
      }
    ],
    "authentication": [
      {
        "id": "ai-seo-agent-auth",
        "label": "AI SEO Agent"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "uuid": "^13.0.0"
  }
}

